{
  "name": "vue-midi-sequencer",
  "version": "0.1.2",
  "description": "A midi sequencer made with vue.js",
  "author": "shundroid <shundroidk@gmail.com>",
  "main": "component/dist/package.js",
  "files": [
    "component/dist/"
  ],
  "scripts": {
    "build:component": "webpack --config component/webpack.config.js --progress --color",
    "build:component:watch": "webpack --watch --config component/webpack.config.js --progress --color",
    "build:package": "webpack --config component/webpack.prod.conf.js --progress --color",
    "build:example": "webpack --config example/webpack.config.js --progress --color",
    "dev": "webpack-dev-server --hot --config component/webpack.config.js --progress --color --inline",
    "lint": "eslint --ext .js,.vue component example",
    "lint:fix": "eslint --fix --ext .js,.vue component example"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.0.0",
    "eslint-plugin-html": "^3.0.0",
    "html-webpack-plugin": "^2.28.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-loader": "^12.2.1",
    "vue-template-compiler": "^2.3.4",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "vue": "^2.3.4",
    "vue-material": "^0.7.1",
    "vuex": "^2.3.1"
  }
}
